{
  "name": "lifestyletest",
  "version": "0.0.0",
  "license": "MIT",
  "author": {
    "name": "Ville Harjunen",
    "email": "ville.harjunen@frantic.com",
    "url": "https://www.frantic.com/"
  },
  "private": true,
  "scripts": {
    "heroku-postbuild": "yarn build",
    "start": "node dist/server.js --universal",
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "yarn build:client && yarn build:server",
    "prebuild:client": "mkdir dist/public && mkdir dist/server",
    "build:client": "ng build --prod && ng run lifestyletest:server",
    "build:server": "webpack --config webpack.server.config.js --progress",
    "dev": "yarn dev:client & yarn dev:server",
    "dev:client": "ng serve --port 5000 --open --hmr --proxy-config proxy.conf.json --configuration=hmr",
    "dev:server": "nodemon --exec ts-node src/server/app.ts --watch src/server/",
    "sync": "yarn sync:db && yarn sync:navigation",
    "sync:db": "ts-node src/server/helpers/database-init.ts",
    "sync:tips": "ts-node src/server/helpers/fetch-and-save-tips-from-wp.ts",
    "sync:navigation": "ts-node src/server/helpers/fetch-navigation-from-wp.ts",
    "report": "ts-node src/server/helpers/answer-count-calculator.ts & ts-node src/server/helpers/monthly-report-calculator.ts & ts-node src/server/helpers/result-category-calculator.ts",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{html,ts,json,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@angular/animations": "^8.2.6",
    "@angular/cli": "^8.2.6",
    "@angular/common": "^8.2.6",
    "@angular/compiler": "^8.2.6",
    "@angular/compiler-cli": "^8.2.6",
    "@angular/core": "^8.2.6",
    "@angular/forms": "^8.2.6",
    "@angular/platform-browser": "^8.2.6",
    "@angular/platform-browser-dynamic": "^8.2.6",
    "@angular/platform-server": "^8.2.6",
    "@angular/router": "^8.2.6",
    "@angularclass/hmr": "^2.1.3",
    "@biesbjerg/ngx-translate-extract": "^4.2.0",
    "@nguniversal/express-engine": "^8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^8.1.1",
    "@ngx-cache/core": "^7.0.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/express-cluster": "^0.0.0",
    "@types/jasmine": "^3.3.12",
    "@types/node": "^12.0.0",
    "async": "^3.1.0",
    "body-parser": "^1.19.0",
    "chalk": "2.4.2",
    "codelyzer": "^5.0.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "core-js": "3.2.1",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "express-cluster": "0.0.5",
    "fs-extra": "^8.1.0",
    "lint-staged": "^9.2.5",
    "mongoose": "^5.5.6",
    "morgan": "^1.9.1",
    "ng-click-outside": "^5.1.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "rxjs": "^6.5.1",
    "rxjs-compat": "^6.0.0-rc.0",
    "superagent": "^5.0.5",
    "ts-loader": "^6.0.0",
    "ts-node": "^8.1.0",
    "tslib": "^1.9.3",
    "tslint": "^5.16.0",
    "typescript": "3.5",
    "web-animations-js": "^2.3.1",
    "webpack-shell-plugin": "^0.5.0",
    "zone.js": "0.9"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.4",
    "husky": "^3.0.5",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "prettier": "^1.17.0",
    "protractor": "~5.4.2",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.2"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  }
}
